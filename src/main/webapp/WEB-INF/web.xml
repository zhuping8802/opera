<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
		<servlet-name>DisplayChart</servlet-name>
		<servlet-class>
			org.jfree.chart.servlet.DisplayChart
		</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>BarServlet</servlet-name>
		<servlet-class>org.ping.chart.BarServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PieServlet</servlet-name>
		<servlet-class>org.ping.chart.PieServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>DisplayChart</servlet-name>
		<url-pattern>/DisplayChart</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>BarServlet</servlet-name>
		<url-pattern>/servlet/BarServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PieServlet</servlet-name>
		<url-pattern>/servlet/PieServlet</url-pattern>
	</servlet-mapping>
	
</web-app>
